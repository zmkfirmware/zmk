#include <dt-bindings/zmk/keys.h>
#include <behaviors.dtsi>
#include <dt-bindings/zmk/kscan_mock.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &kp B &tog 1
                &mo 2 &to 1>;
        };

        lower_layer {
            bindings = <
                &kp C_NEXT &trans
                &kp L  &kp J>;
        };

        raise_layer {
            bindings = <
                &kp W &tog 2
                &tog_off 2  &mo 2>;
        };
    };
};
