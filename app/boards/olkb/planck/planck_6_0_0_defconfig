CONFIG_SYS_CLOCK_HW_CYCLES_PER_SEC=72000000

CONFIG_PINCTRL=y

# enable GPIO
CONFIG_GPIO=y

# clock configuration
CONFIG_CLOCK_CONTROL=y

CONFIG_ZMK_USB=y

